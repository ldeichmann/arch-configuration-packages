post_install() {
  echo "Enabling gdm"
  systemctl enable gdm.service
  echo "Enabling network manager"
  systemctl enable NetworkManager.service
  echo "Updating dconf"
  dconf update
}

post_upgrade() {
  echo "Updating dconf"
  dconf update
}

post_remove() {
  echo "Updating dconf"
  dconf update
}
